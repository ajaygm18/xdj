{
  "data_dir": "data",
  "results_dir": "results_fast",
  "symbol": "^GSPC",
  "data_years": 10,
  "use_paper_compliant_labeling": true,
  "use_exact_40_indicators": true,
  "cae": {
    "hidden_dim": 128,
    "encoding_dim": 32,
    "dropout": 0.1,
    "lambda_reg": 5e-4,
    "epochs": 50,
    "batch_size": 64,
    "learning_rate": 1e-3
  },
  "training": {
    "window_length": 20,
    "step_size": 1,
    "epochs": 100,
    "batch_size": 32,
    "learning_rate": 1e-3,
    "patience": 20
  },
  "plstm_tal": {
    "hidden_size": 64,
    "num_layers": 1,
    "dropout": 0.1,
    "bidirectional": false,
    "activation": "tanh"
  },
  "eemd": {
    "n_ensembles": 20,
    "noise_scale": 0.2,
    "max_imfs": 10,
    "tolerance": 0.2,
    "w": 7
  },
  "baselines": {
    "epochs": 50,
    "dropout": 0.1,
    "cnn_filters": 32,
    "lstm_hidden": 32,
    "rf_trees": 50,
    "rf_depth": 10,
    "rf_min_split": 5,
    "svm_C": 1.0,
    "svm_gamma": "scale"
  }
}