{
  "data_dir": "data",
  "results_dir": "results",
  "symbol": "^GSPC",
  "market": "SP500",
  "use_bayesian_optimization": false,
  "bayesian_n_calls": 50,
  "bayesian_quick_mode": false,
  "data_years": 10,
  "use_paper_timeframe": false,
  "start_date": "2015-01-01",
  "end_date": "2025-01-01",
  "use_paper_compliant_labeling": true,
  "use_exact_40_indicators": true,
  "eemd": {
    "n_ensembles": 100,
    "noise_scale": 0.2,
    "w": 7
  },
  "cae": {
    "hidden_dim": 64,
    "encoding_dim": 16,
    "dropout": 0.1,
    "lambda_reg": 1e-4,
    "epochs": 100,
    "batch_size": 32,
    "learning_rate": 1e-3
  },
  "training": {
    "window_length": 20,
    "step_size": 1,
    "epochs": 100,
    "batch_size": 32,
    "learning_rate": 1e-3,
    "optimizer": "adamax",
    "patience": 20
  },
  "plstm_tal": {
    "hidden_size": 64,
    "num_layers": 1,
    "dropout": 0.1,
    "activation": "tanh"
  },
  "baselines": {
    "epochs": 100,
    "dropout": 0.1,
    "cnn_filters": 32,
    "lstm_hidden": 64,
    "rf_trees": 100,
    "rf_depth": 10,
    "rf_min_split": 5,
    "svm_C": 1.0,
    "svm_gamma": "scale"
  }
}