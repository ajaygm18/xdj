{
  "data_dir": "data",
  "results_dir": "results",
  "symbol": "^GSPC",
  "market": "SP500",
  "use_bayesian_optimization": false,
  "bayesian_n_calls": 50,
  "bayesian_quick_mode": false,
  "data_years": 10,
  "use_paper_timeframe": false,
  "start_date": "2015-01-01",
  "end_date": "2025-01-01",
  "use_paper_compliant_labeling": true,
  "use_exact_40_indicators": true,
  "eemd": {
    "n_ensembles": 8,
    "noise_scale": 0.2,
    "w": 7
  },
  "cae": {
    "hidden_dim": 128,
    "encoding_dim": 32,
    "dropout": 0.2,
    "lambda_reg": 1e-4,
    "epochs": 200,
    "batch_size": 64,
    "learning_rate": 2e-4
  },
  "training": {
    "window_length": 30,
    "step_size": 1,
    "epochs": 300,
    "batch_size": 64,
    "learning_rate": 1e-4,
    "optimizer": "adamw",
    "patience": 100,
    "warmup_epochs": 50,
    "gradient_clip": 1.0,
    "weight_decay": 1e-5
  },
  "plstm_tal": {
    "hidden_size": 128,
    "num_layers": 3,
    "dropout": 0.3,
    "activation": "tanh",
    "bidirectional": true,
    "attention_heads": 8,
    "use_residual": true,
    "use_layer_norm": true,
    "attention_dropout": 0.2
  },
  "baselines": {
    "epochs": 300,
    "dropout": 0.2,
    "cnn_filters": 128,
    "lstm_hidden": 128,
    "rf_trees": 200,
    "rf_depth": 15,
    "rf_min_split": 3,
    "svm_C": 2.0,
    "svm_gamma": "scale"
  }
}